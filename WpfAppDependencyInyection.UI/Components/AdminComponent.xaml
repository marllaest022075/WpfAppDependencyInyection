<UserControl x:Class="WpfAppDependencyInyection.UI.Components.AdminComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfAppDependencyInyection.UI.Components" 
             xmlns:viewmodels="clr-namespace:WpfAppDependencyInyection.UI.ViewModels" 
             d:DataContext="{d:DesignInstance Type=viewmodels:AdminViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="900">
    <Grid>
        <TextBlock Text="{Binding Path=Mensage}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="50"/>
    </Grid>
</UserControl>
